{
  "name": "greencrowd",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"node src/ws/server.js\" \"next dev\"",
    "build": "next build",
    "start": "concurrently \"node src/ws/server.js\" \"next start\"",
    "lint": "next lint",
    "migrate:reset:local": "dotenv -e .env.local -- npx prisma migrate reset",
    "migration:format": "dotenv -e .env.local -- npx prisma format",
    "migration:dev": "dotenv -e .env.local -- npx prisma migrate dev",
    "migration:generate": "dotenv -e .env.local -- npx prisma generate --schema=./prisma/schema.prisma",
    "migration:deploy": "dotenv -e .env.local -- npx prisma migrate deploy"
  },
